CREATE TABLE `medicalrecord` (
  `medicalRecordId` int NOT NULL AUTO_INCREMENT,
  `diagnosis` varchar(225) DEFAULT NULL,
  `diagnosisNotes` varchar(225) DEFAULT NULL,
  `medicalResult` varchar(225) DEFAULT NULL,
  `medicationsPrescribed` varchar(225) DEFAULT NULL,
  `date` date DEFAULT NULL,
  `employeeId` int DEFAULT NULL,
  `patientId` int DEFAULT NULL,
  PRIMARY KEY (`medicalRecordId`)
) 
